# Golf Ryder Cup App - Environment Variables
# Copy this file to .env.local and fill in your values

# ======================================
# REQUIRED - Supabase Configuration
# ======================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# ======================================
# OPTIONAL - API Keys
# ======================================

# Golf Course API (for course lookups)
GOLF_COURSE_API_KEY=
NEXT_PUBLIC_GOLF_COURSE_API_KEY=

# GHIN API (for handicap lookups)
GHIN_API_KEY=

# RapidAPI (alternative golf data source)
RAPIDAPI_KEY=

# ======================================
# OPTIONAL - AI Features
# ======================================

# Anthropic Claude API (for scorecard OCR)
ANTHROPIC_API_KEY=

# OpenAI API (alternative OCR provider)
OPENAI_API_KEY=
