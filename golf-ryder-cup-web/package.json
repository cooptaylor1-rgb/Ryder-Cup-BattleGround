{
  "name": "golf-ryder-cup-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "qa:sim": "playwright test e2e/qa-simulation.spec.ts --workers=4 --reporter=list",
    "test:pw": "playwright test",
    "test:pw:headed": "playwright test --headed",
    "test:smoke": "playwright test --grep '@smoke'",
    "test:regression": "playwright test --grep '@regression'",
    "test:nightly": "playwright test --grep '@nightly'",
    "test:chaos": "CHAOS_ENABLED=true playwright test tests/e2e/chaos/ --workers=1",
    "test:fuzz": "playwright test tests/e2e/fuzz/ --workers=1",
    "test:fuzz:ci": "FUZZ_ACTIONS=50 playwright test tests/e2e/fuzz/ --workers=1",
    "test:journeys": "playwright test tests/e2e/journeys/",
    "test:simulate:1000": "npx tsx scripts/testing/simulate-1000.ts",
    "test:seed:small": "npx tsx scripts/testing/seed-small.ts",
    "test:seed:large": "npx tsx scripts/testing/seed-large.ts",
    "test:db": "vitest run src/__tests__/schemaVerification.test.ts",
    "db:lint": "vitest run src/__tests__/schemaVerification.test.ts",
    "db:diff": "supabase db diff",
    "db:push": "supabase db push",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@sentry/nextjs": "^10.34.0",
    "@supabase/supabase-js": "^2.90.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "framer-motion": "^12.26.2",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-pwa": "^5.6.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.4",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^27.4.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }
}
